@model Restaurant

@{
    ViewBag.Title = Model.Name;
}

<section class="panel panel-default">
    <h3 class="panel-heading">@Model.Name</h3>
    <div class="panel-body">
        <p>Id: @Model.Id</p>
        <p>Cuisine: @Model.Cuisine</p>
    </div>
</section>

<a class="btn btn-default" asp-action="@nameof(HomeController.Index)">Back to home</a>
